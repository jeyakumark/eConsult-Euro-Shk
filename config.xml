        versionCode = "10"
        version = "2.0.0" >
	 <gap:splash src="images/splash.png" gap:platform="ios" width="1024" height="768" />
	 <gap:platform name="ios" />
  
    <gap:plugin name="org.apache.cordova.camera" />
    <gap:plugin name="org.apache.cordova.core.file" />
